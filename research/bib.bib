@book{faridi_most_2020,
	title = {The most commonly used greenhouse cooling technologies: {Conventional} against modern methods},
	shorttitle = {The most commonly used greenhouse cooling technologies},
	abstract = {The most important goal of a greenhouse is high yield production outside the cultivation season which is possible by maintaining optimum conditions specially temperature at each stage of a crop's growth. In the warm seasons, temperature in the greenhouse may exceed the optimal temperature for cultivated plants. In order to have the desired temperature for a greenhouse in summer, it must be cooled as quickly as it gains heat from the ambient. For reducing the internal heat of the greenhouse and heat discharge, the greenhouse needs to be coupled with a proper cooling system based on the climatic conditions of the region. This article focuses on the current greenhouse cooling methods and compares them with modern, renewable energy-based methods with the aim of using clean energy, reducing the possible potential of environmental damage, as well as reducing energy consumption and also using winter energy storage like solar energy and applying for the greenhouse cooling demands in warm seasons.},
	author = {Faridi, Hamideh and Arabhosseini, Akbar and Zarei, Ghasem and Okos, M.},
	month = jan,
	year = {2020},
